<% include partials/header %>
<% include partials/menu %>

<div class="container">
  <div class="row">

  <form action=/add method="post">

    <div class="form-group row">
      <label for="inputEmail3" class="col-sm-2 col-form-label">Nama</label>
      <div class="col-sm-10">
        <input type="text" class="form-control" name="string" placeholder="Nama: Tipe data string">
      </div>
    </div>


    <div class="form-group row">
      <label for="inputPassword3" class="col-sm-2 col-form-label">Umur</label>
      <div class="col-sm-10">
        <input type="number" class="form-control" name="integer" step="1" placeholder="Umur tipe data integer">
      </div>
    </div>

    <div class="form-group row">
      <label for="inputPassword3" class="col-sm-2 col-form-label">Tinggi</label>
      <div class="col-sm-10">
        <input type="number" class="form-control" name="float" step="0.01" placeholder="Tinggi tipe data Float">
      </div>
    </div>

    <div class="form-group row">
      <label for="inputPassword3" class="col-sm-2 col-form-label">Tanggal Lahir</label>
      <div class="col-sm-10">
        <input type="date" class="form-control" name="date" >
      </div>
    </div>

    <div class="form-group row">
      <label for="inputPassword3" class="col-sm-2 col-form-label">Status</label>
      <div class="col-sm-10">
        <select class="form-control" name="boolean">
          <option selected >Status Tipe Boolean</option>
          <option value="true">True</option>
          <option value=false>False</option>
        </select>
      </div>
    </div>


    <div class="form-group row">
      <div class="col-sm-10">
        <button type="submit" class="btn btn-primary">Simpan</button>
      </div>
    </div>

  </form>
</div>
</div>

</div>
<% include partials/footer %>
