<% include partials/header %>
<form action="/add" method="post" class="form-horizontal">

  <div class="col-xs-9">
    <div class="form-horizontal">
      <h2>Add</h2>
      <hr>

      <div class="form-group form-group-sm">
        <label class="col-sm-2 control-label" > Name </label>
        <div class="col-sm-10">
          <input class="form-control" type="text" id="name" name="name" placeholder="Name">
        </div>
      </div>

      <div class="form-group form-group-sm">
        <label class="col-sm-2 control-label">Members</label>
        <div class="col-sm-10">
          <% for(var i=0; i < user.length; i++) { %>
          <input type="checkbox" name="member" value="<%= user[i].userid%>"> <%= user[i].fristname%><br>
          <% } %>
        </div>
      </div>

      <div class="form-group form-group-sm">
        <label class="col-sm-2 control-label" for="formGroupInputSmall"></label>
        <div class="col-sm-10">
        <button  style="margin-top : 30px" type="submit" name="button" class="btn btn-primary"> Save </button>

        </div>

      </div>

    </div>
  </div>
</form>
<% include partials/footer %>
